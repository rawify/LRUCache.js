{
    "name": "@rawify/lrucache",
    "title": "LRUCache.js",
    "version": "0.0.3",
    "description": "The RAW Least Recently Used (LRU) cache with optional TTL support",
    "homepage": "https://github.com/rawify/LRUCache.js",
    "bugs": "https://github.com/rawify/LRUCache.js/issues",
    "keywords": [
        "lru",
        "lru-cache",
        "cache",
        "ttl",
        "map",
        "least-recently-used",
        "memory-cache",
        "expiration"
    ],
    "private": false,
    "main": "./dist/lrucache.js",
    "module": "./dist/lrucache.mjs",
    "browser": "./dist/lrucache.min.js",
    "unpkg": "./dist/lrucache.min.js",
    "types": "./lrucache.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./lrucache.d.mts",
                "default": "./dist/lrucache.mjs"
            },
            "require": {
                "types": "./lrucache.d.ts",
                "default": "./dist/lrucache.js"
            },
            "browser": {
                "types": "./lrucache.d.ts",
                "default": "./dist/lrucache.min.js"
            },
            "default": {
                "types": "./lrucache.d.ts",
                "default": "./dist/lrucache.js"
            }
        },
        "./package.json": "./package.json"
    },
    "typesVersions": {
        "<4.7": {
            "*": [
                "lrucache.d.ts"
            ]
        }
    },
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/rawify/LRUCache.js.git"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/rawify"
    },
    "author": {
        "name": "Robert Eisele",
        "email": "robert@raw.org",
        "url": "https://raw.org/"
    },
    "license": "MIT",
    "engines": {
        "node": "*"
    },
    "directories": {
        "test": "tests"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "crude-build LRUCache"
    },
    "devDependencies": {
        "crude-build": "^0.1.2"
    }
}
